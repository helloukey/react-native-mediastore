    "react-native-media-library",
    "react-native",
    "react-native-fs",
    "react-native-file-access",
    "react-native-mediastore",
    "react-native-scoped-storage",
    "react-native-file-system"
  ],
  "repository": "https://github.com/helloukey/react-native-mediastore/",
  "author": "Kunal Ukey - https://github.com/helloukey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/helloukey/react-native-mediastore/issues"
  },
  "homepage": "https://github.com/helloukey/react-native-mediastore#readme",
  "devDependencies": {},
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "codegenConfig": {
    "name": "ReactNativeMediaStoreSpec",
    "type": "modules",
